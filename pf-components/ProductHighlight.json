{
  "key": "ProductFocus",
  "name": "ProductFocus",
  "description": "Highlight a product in a fancy way",
  "elements": [
    {
      "name": "Color",
      "selector": "h2 > span:last-child",
      "properties": [
        {
          "name": "Text Color",
          "values": [
            {
              "name": "Dark",
              "value": "text-dark",
              "display": "#333"
            },
            {
              "name": "Light",
              "value": "text-light",
              "display": "#f0f0f0"
            }
          ],
          "ui": "ui-colorgroup"
        }
      ]
    }
  ]
}
